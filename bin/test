#!/usr/bin/env zsh
set -euo pipefail
echo "Running FlatshareNaija smoke tests..."
flatshareNaija init
flatshareNaija user add "TestUser" "test@example.com" "+2348012345678" "Abuja"
flatshareNaija pref set "TestUser" "600000" "Abuja" "Wuse Zone 4" "remote"
flatshareNaija listing add "Mini-flat Wuse Zone 4" "Self-con, borehole, gen" "Abuja" "Wuse Zone 4" 500000
flatshareNaija match "TestUser"
echo "âœ… All smoke tests passed"
